&pio {
    sar_int_pin {
        sar_int_default: sar_int_default {
            /* active state */
            pinmux = <PINMUX_GPIO12__FUNC_GPIO12>;
            slew-rate = <2>;
            bias-pull-up;
        };
    };
};

&mt_pmic_vio28_ldo_reg {
        regulator-name = "vio28";
        regulator-min-microvolt = <3000000>;
        regulator-max-microvolt = <3000000>;
        regulator-enable-ramp-delay = <1920>;
};

&i2c0 {
    status = "ok";
    Semtech_sx933x@28 {
        status = "ok";
        compatible = "Semtech,sx933x";
        reg = <0x28>;
        cap_vdd-supply = <&mt_pmic_vio28_ldo_reg>;
        interrupt-parent = <&pio>;
        interrupts = <12 IRQ_TYPE_LEVEL_HIGH 12 0>;
        Semtech,nirq-gpio = <&pio 12 0x0>;
        pinctrl-names = "sar_int_default";
        pinctrl-0 = <&sar_int_default>;
        /*use PH3/4 as the reference sensor
        set it to 0xff if the ref-phases-x is not used*/
        reinit-on-i2c-failure;
        reinit-on-irq-failure;
        Semtech,button-flag = <0x07>;
        Semtech,ref-phases-a = <3>;
        Semtech,ref-phases-b = <4>;
        Semtech,reg-num = <83>;
        Semtech,reg-init = <
